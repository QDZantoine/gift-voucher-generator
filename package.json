{
  "name": "influences",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "db:studio": "node -r dotenv/config node_modules/.bin/prisma studio",
    "db:migrate": "node -r dotenv/config node_modules/.bin/prisma migrate dev",
    "db:generate": "node -r dotenv/config node_modules/.bin/prisma generate",
    "db:push": "node -r dotenv/config node_modules/.bin/prisma db push",
    "create-admin": "tsx -r dotenv/config scripts/create-admin.ts",
    "clear-gift-cards": "tsx -r dotenv/config scripts/clear-gift-cards.ts",
    "clean": "rm -rf .next node_modules/.cache dist out",
    "clean:console": "node scripts/clean-console-logs.js",
    "analyze": "ANALYZE=true next build",
    "postinstall": "node scripts/postinstall.js",
    "postinstall:prod": "node -r dotenv/config node_modules/.bin/prisma generate",
    "vercel-build": "node -r dotenv/config node_modules/.bin/prisma generate && next build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@lexical/react": "^0.37.0",
    "@lexical/rich-text": "^0.37.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stripe/stripe-js": "^8.0.0",
    "bcryptjs": "^2.4.3",
    "better-auth": "^1.3.8",
    "better-sqlite3": "^12.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "lexical": "^0.37.0",
    "lucide-react": "^0.545.0",
    "next": "15.5.5",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "puppeteer": "^24.24.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.2",
    "resend": "^6.1.3",
    "sonner": "^1.7.1",
    "stripe": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-loader": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "prisma": "^7.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
